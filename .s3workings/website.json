{
    "IndexDocument": {
        "Suffix": "index.html"
    },
    "ErrorDocument": {
        "Key": "404.html"
    },
    "RoutingRules": [
        {
            "Redirect": {
                "ReplaceKeyWith": "index.html"
            },
            "Condition": {
                "KeyPrefixEquals": "/"
            }
        }
    ]
}
